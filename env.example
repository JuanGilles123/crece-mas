# Configuración de Supabase
# Obtén estos valores desde tu proyecto en https://supabase.com

# URL del proyecto de Supabase
REACT_APP_SUPABASE_URL=your_supabase_project_url

# Clave anónima de Supabase
REACT_APP_SUPABASE_ANON_KEY=your_supabase_anon_key

# Configuración de la aplicación
REACT_APP_APP_NAME=Crece Más
REACT_APP_APP_VERSION=1.0.0

# Configuración de desarrollo
REACT_APP_DEBUG=false
REACT_APP_LOG_LEVEL=info

# Configuración de storage
REACT_APP_STORAGE_BUCKET_RECIBOS=recibos
REACT_APP_STORAGE_BUCKET_LOGOS=logos
REACT_APP_STORAGE_BUCKET_PRODUCTOS=productos

# Configuración de límites
REACT_APP_MAX_FILE_SIZE=10485760
REACT_APP_APP_MAX_IMAGE_SIZE=2097152
REACT_APP_MAX_PRODUCTS_PER_USER=1000

# Configuración de PDF
REACT_APP_PDF_QUALITY=0.92
REACT_APP_PDF_FORMAT=A4

# Configuración de notificaciones
REACT_APP_TOAST_DURATION=4000
REACT_APP_TOAST_POSITION=top-right
